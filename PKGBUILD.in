pkgname={name}
pkgver='{pkgver}'
pkgrel=1
pkgdesc='{description}'
arch=('x86_64')
license=('custom')
url={url}

depends=({dependencies})
conflicts=({conflicts})
provides=({provides})

groups=(zivid-all)

source=({source})
sha256sums=({sha256sum})

options=(!strip)

package() {{
    bsdtar -xf data.tar.* -C "${{pkgdir}}"
}}
